{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nadirhamid/jwt-auth"
        },
        {
            "type": "vcs",
            "url": "https://github.com/nadirhamid/vultr-client-php"
        },

        {
             "type": "composer", 
             "url": "https://packagist.org"
        },
        { "packagist": false }
    ],
    "require": {
        "php": ">=5.5.9",
        "psr/log": "1.0.0",
        "laravel/framework": "5.1.*",
        "yajra/laravel-datatables-oracle" : "~5.0",
        "barryvdh/laravel-debugbar": "^2.0@dev",
        "laravelcollective/html": "5.1.*",
        "cviebrock/eloquent-sluggable": "3.1.4",
        "dingo/api": "1.0.x@dev",
        "tymon/jwt-auth": "0.5.13",
        "doctrine/dbal": "^2.9",
        "ramsey/uuid": "^3.8.0",
        "telnyx/telnyx-php": "^0.0.1",
        "stripe/stripe-php": "^6.43",
        "nubs/random-name-generator": "^2.1",
        "giggsey/libphonenumber-for-php": "^8.10",
        "twilio/sdk": "^5.35",
        "paypal/rest-api-sdk-php": "1.13.0",
        "linkorb/vultr-client-php": "dev-update",
        "google/cloud": "^0.112.0",
        "michelf/php-markdown": "^1.8",
        "guzzlehttp/guzzle": "^6.2",
        "barryvdh/laravel-dompdf": "v0.8.2",
        "mobiledetect/mobiledetectlib": "^2.8",
        "google/apiclient": "^2.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "hackzilla/password-generator": "dev-master",
        "messagebird/php-rest-api": "dev-master",
        "fideloper/proxy": "^3.3",
        "sentry/sentry-laravel": "1.8.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
